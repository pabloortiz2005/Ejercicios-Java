package ejercicios;

public class ej3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		
		int a = 1;
		double d = 1.0;
		
		
		a= 46/9;
		System.out.println("a: " + a);
		
		a= 46%9 + 4 * 4 - 2;
		System.out.println("a: " + a);
		
		a= 45 + 43 % 5 * (23 * 3 % 2);
		System.out.println("a: " + a);
		
		a= 45 + 1 + 45 * 50 % a--;
		System.out.println("a: " + a);
		
		a= 45 + 1 + 45 * 50 % (a--);
		System.out.println("a: " + a);
		
		d= 1.5 * 3 + (++d);
		System.out.println("d: " + d);
		
		d= 1.5 * 3 + d++;
		System.out.println("d: " + d);
		
		a%= 3 / a +3 ;
		System.out.println("a: " + a);
		
		
		
	}

}
