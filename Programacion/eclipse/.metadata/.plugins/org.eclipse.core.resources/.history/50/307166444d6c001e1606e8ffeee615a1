		double decimal1 = 2;
		double decimal2 = 7.8;
		double decimal3 = 9.0;
		
		decimal1 = (decimal2 * decimal3)
		decimal2 = (decimal1 + decimal3)
		decimal3 = (decimal1 * decimal2)
		
		System.out.println("Decimal 1: " decimal1);
		System.out.println("Decimal 2: " decimal2);
		System.out.println("Decimal 3: " decimal3);
		
		
		
		
		
		
	}

}
